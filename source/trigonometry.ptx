              \node[anchor = north east] at (45:3) {$\frac{\pi}{4}$}; %pi/4
              \filldraw (45:3) circle (.05cm);
              \node[anchor = south west] at (45:3)  {$\left(\frac{1}{\sqrt{2}},\frac{1}{\sqrt{2}}\right)$};
